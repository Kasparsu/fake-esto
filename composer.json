{
    "name": "kasparsu/fake-esto",
    "description": "Faker providers for estonian language",
    "keywords": [
        "faker",
        "estonian",
        "fake-estonian",
        "esto-fake",
        "fake-esto",
        "estonian-fake",
        "dummy-text"
    ],
    "authors": [
        {
            "name": "Kaspar Martin Suursalu",
            "email": "kasparsu@gmail.com"
        }
    ],
    "type": "library",
    "license": "WTFPL",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Faker\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faker\\Tests\\Provider\\": "tests/Faker"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit -c phpunit.xml"
        ],
        "test-coverage": [
            "./vendor/bin/phpunit --coverage-text -c phpunit.xml"
        ]
    }
}
